{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier"
  ],
  "rules": {
    "import/no-unresolved": [0],
    "max-len": ["error", {
      "code": 80,
      "ignoreUrls": true
    }],
    "no-unexpected-multiline": ["error"]
  },
  "globals": {
    "document": true,
    "window": true
  }
}